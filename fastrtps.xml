                            <udpv4>
                                <address>192.168.100.100</address>
                            </udpv4>
                        </locator>
                    </initialPeersList>
                </builtin>
                <defaultUnicastLocatorList>
                    <locator>
                        <udpv4>
                            <address>192.168.100.52</address>
                        </udpv4>
                    </locator>
                </defaultUnicastLocatorList>
                <useBuiltinTransports>true</useBuiltinTransports>
            </rtps>
        </participant>
    </profiles>
</dds>
